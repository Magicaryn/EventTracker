<div class="alert"></div>
<div class="login-card">
    <h1 class="login-title" style="font-family: 'Roboto Condensed',sans-serif; font-weight:bolder;">Login</h1>
    <form class="login-form">
        <div class="row">
        <div class="form-group col-sm-6 mb-3">
            <label for="username-login">Username:</label>
            <input class="form-input form-control" class="form-label" type="text" id="username-login" />
       </div>
       </div>
       <div class="row">
        <div class="form-group col-sm-6 mb-3">
            <label for="password-login">Password:</label>
            <input class="form-input form-control" class="form-label" type="password" id="password-login" />
        </div>
        <div class="form-group mb-3">
             <button class="btn btn-outline-light btn-dark" type="submit">Login</button>
        </div>
        </div>
    </form>
</div>
